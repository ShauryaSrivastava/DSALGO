public static void RotateArray(int[] nums,int k)	{
		int[] a = new int[nums.Length];
        for(int i=0;i< nums.Length;i++)
        {
            a[(i+k) % nums.Length] = nums[i]; 
        }
	for(int i=0;i<nums.Length;i++)
	{
		nums[i] = a[i];
		Console.WriteLine(nums[i]); // Print Rotated Arrays
	}
			
}
